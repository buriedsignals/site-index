{
  "version": 2,
  "routes": [
    {
      "src": "/([^/]+)",
      "status": 302,
      "headers": {
        "Location": "/$1/"
      }
    },
    {
      "src": "/about/(.*)",
      "dest": "https://buried-signals-about.vercel.app/$1"
    },
    {
      "src": "https:///some-otheer-site.com/article/(.*)",
      "dest": "undefined/$1"
    }
  ]
}